<style>
body {
  margin: 0;
  font-family: Arial, Helvetica, sans-serif;
  background: #f4f6f9;
}

/* Top bar */
header {
  background: #1f3a5f;
  color: white;
  padding: 14px 20px;
  font-size: 20px;
  font-weight: bold;
}

/* Layout */
.container {
  display: flex;
  height: calc(100vh - 52px);
}

/* Sidebar */
nav {
  width: 260px;
  background: #ffffff;
  border-right: 1px solid #ddd;
  padding: 10px;
}

nav h3 {
  font-size: 14px;
  color: #666;
  margin: 10px 0;
}

.course {
  padding: 12px;
  margin-bottom: 6px;
  background: #f7f7f7;
  border-radius: 6px;
  cursor: pointer;
  transition: background 0.2s;
}

.course:hover {
  background: #e6ecf3;
}

/* Main area */
main {
  flex: 1;
  background: #fff;
  position: relative;
}

iframe {
  width: 100%;
  height: 100%;
  border: none;
}

/* Placeholder */
#welcome {
  padding: 40px;
  color: #444;
}
</style>
