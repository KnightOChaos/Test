function initCounter(){if(document.querySelector(".m7-old")){let e,n=document.querySelectorAll("[data-num]");if(!n.length)return;function t(){window.scrollY+window.innerHeight>document.querySelector(".counter-group").offsetTop&&!e&&(n.forEach(function(e){var n=e.dataset.num;new Odometer({el:e,zeroFlag:!0,format:"dd",duration:9e3,numberLength:n.toString().length}),e.innerHTML=n,e.addEventListener("odometerdone",function(){})}),e=!0)}window.addEventListener("load",t),window.addEventListener("scroll",t)}if(document.querySelector(".m7.count-anim")){let e,n=document.querySelectorAll("[data-count]");if(n.length){function t(){window.scrollY+window.innerHeight-250>document.querySelector(".m7.count-anim").offsetTop&&!e&&(n.forEach(o=>{let r=()=>{var e=parseInt(o.getAttribute("data-count")),n=+o.innerText,t=e<200?1:Math.trunc(e/200);n<e?(o.innerText=n+t,setTimeout(r,15)):o.innerText=e};r()}),e=!0)}window.addEventListener("load",t),window.addEventListener("scroll",t)}}}initCounter()